[
  {
    "__comment" : "Start by making any vanilla minecraft biome default to oak",
    "select": { "name": "minecraft:.*" },
    "apply": {
      "species" : "oak",
      "density": 0,
      "chance" : "standard"
    }
  },
  {
    "__comment": "Cancel standard tree features from the overworld.",
    "select": { "name": "minecraft.*", "tag": "forge:is_overworld" },
    "cancellers": {
      "type": "tree",
      "namespace": "minecraft"
    }
  },
  {
    "__comment" : "Rivers have no chance of having trees",
    "select": { "tag": "minecraft:is_river" },
    "apply": {
      "chance": 0.0
    }
  },
  {
    "__comment" : "Default for a Forest which generates a Birch Tree 1/5 of the time.",
    "select": { "tag": "minecraft:is_forest", "name": "minecraft:.*" },
    "apply": {
      "species" : {
        "random" : {
          "oak" : 4,
          "birch" : 1
        }
      },
      "density": [ 1 ],
      "chance": 1.0,
      "forestness" : 1.0
    }
  },
  {
    "select": "minecraft:flower_forest",
    "apply": {
      "density": [ 0.6 ]
    }
  },
  {
    "__comment" : "Anything cold will have spruce trees except oceans, peaks, and ice spikes",
    "select": { "tags": [ "forge:is_cold", "!forge:is_ocean", "!forge:is_peak" ], "names": [ "minecraft:.*", "!minecraft:ice_spikes" ] },
    "apply": {
      "species" : "spruce"
    }
  },
  {
    "__comment" : "Any wasteland will be treeless",
    "select": { "tag": "forge:is_wasteland", "name": "minecraft:.*" },
    "apply": {
      "species" : "..."
    }
  },
  {
    "__comment" : "Generates an alternative oak style in swamps",
    "select": { "tag": "forge:is_swamp", "name": "minecraft:.*" },
    "apply": {
      "species" : "swamp_oak",
      "density": [ 0.2 ],
      "chance": 0.75
    }
  },
  {
    "select": { "tag": "forge:is_savanna", "name": "minecraft:.*" },
    "apply": {
      "species" : "acacia",
      "density": [ 0.1 ]
    }
  },
  {
    "select": { "name": "minecraft:windswept_savanna.*" },
    "apply": {
      "density": [ 0.2 ]
    }
  },
  {
    "select": { "tag": "minecraft:is_jungle", "name": "minecraft:.*" },
    "apply": {
      "species" : {
        "random" : {
          "jungle" : 5,
          "mega_jungle" : 1
        }
      },
      "density": [ 2 ],
      "forestness" : 1.0
    }
  },
  {
    "select": { "name": "minecraft:.*sparse_jungle" },
    "apply": {
      "density": [ 0.2 ],
      "chance": 0.5
    }
  },
  {
    "__comment" : "Mushroom Islands",
    "select": { "tag": "forge:is_mushroom",	"name": "minecraft:.*" },
    "apply": {
      "species" : {
        "random" : {
          "red_mushroom" : 4,
          "brown_mushroom" : 3
        }
      },
      "chance": 0.66
    },
    "cancellers": {
      "type": "mushroom",
      "namespace": "minecraft"
    }
  },
  {
    "select": { "tag": "forge:is_coniferous", "name": "minecraft:.*" },
    "apply": {
      "species" : "spruce",
      "density": [ 1 ],
      "chance": 1
    }
  },
  {
    "__comment" : "Roofed forests are handled specially. Multipass tries to close canopy as much as possible",
    "select": { "tag": "forge:is_spooky", "name": "minecraft:.*" },
    "apply": {
      "species" : "dark_oak",
      "density" : { "math": { "div" : [ "noise", 3.0 ] } },
      "multipass" : true
    }
  },
  {
    "select": { "name": "minecraft:.*beach.*" },
    "apply": {
      "chance": 0.0
    }
  },
  {
    "__comment": "Any biome with 'birch' in the name is 100% birch",
    "select": { "name": "minecraft:.*birch.*" },
    "apply": {
      "species" : "birch"
    }
  },
  {
    "__comment": "Old growth birch forest has tall birch trees",
    "select": { "name": "minecraft:old_growth_birch_forest" },
    "apply": {
      "species" : "tall_birch"
    }
  },
  {
    "__comment": "1/25 chance to spawn apple trees in any minecraft biome that ends in 'plains'",
    "select": { "name": "minecraft:.*plains" },
    "apply": {
      "species" : {
        "random" : {
          "oak": 24,
          "apple_oak": 1
        }
      },
      "density": 0,
      "chance": 0.03
    }
  },
  {
    "__comment": "All meadow biomes rarely generate an oak or tall birch tree",
    "select": { "name": "minecraft:meadow" },
    "apply": {
      "species": {
        "random": {
          "oak": 1,
          "tall_birch": 1
        }
      },
      "density": [ 0.1 ],
      "chance": 0.002
    }
  },
  {
    "__comment" : "All lush caves can generate subterranean azalea trees",
    "select": { "name": "minecraft:lush_caves" },
    "apply": {
      "subterranean": true,
      "species" : {
        "random" : {
          "azalea" : 1,
          "flowering_azalea" : 1
        }
      },
      "chance": 0.66
    }
  },
  {
    "_comment": "Windswept Forests are denser and spawn more trees.",
    "select": "minecraft:windswept_forest",
    "apply": {
      "density": [ 0.7 ],
      "chance": 0.6
    }
  },
  {
    "__comment": "Mega taiga has giant spruce trees.",
    "select": { "name": "minecraft:old_growth_spruce_taiga" },
    "apply": {
      "species" : "mega_spruce"
    }
  },
  {
    "__comment": "Giant tree taiga has giant spruce trees, but also is less leaf-dense than giant spruce taiga.",
    "select": { "name": "minecraft:old_growth_pine_taiga" },
    "apply": {
      "species" : {
        "random" : {
          "mega_spruce" : 2,
          "spruce" : 1
        }
      }
    }
  },
  {
    "__comment" : "Wooded Mesas have a chance of generating oak on the dirt patches.",
    "select": { "name": "minecraft:wooded_badlands" },
    "apply": {
      "species" : "oak",
      "density": [ 0.5 ],
      "chance": 0.5
    }
  },

  {
    "select": { "name": "minecraft:.*", "tag": "minecraft:is_nether" },
    "apply": {
      "chance": 0.4
    }
  },
  {
    "__comment": "Cancel huge fungus features from namespace 'minecraft' in the nether forests, and set chance.",
    "select": { "name": "minecraft:.*forest", "tag": "minecraft:is_nether" },
    "cancellers": {
      "type": "fungus",
      "namespace": "minecraft"
    },
    "apply": {
      "chance": 1.0
    }
  },
  {
    "select": { "name": "minecraft:crimson_forest" },
    "apply": {
      "species" : {
        "random" : {
          "crimson" : 4,
          "mega_crimson" : 1
        }
      },
      "density": [ 2.0, 0.3 ]
    }
  },
  {
    "select": { "name": "minecraft:warped_forest" },
    "apply": {
      "species" : {
        "random" : {
          "warped" : 4,
          "mega_warped" : 1
        }
      },
      "density": [ 2.0, 0.3 ]
    }
  }
]